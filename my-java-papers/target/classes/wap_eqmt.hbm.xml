<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.itsmyjava.blogspot.hibernate.vo.WapEquipment"
		table="wap_eqmt">
		<composite-id>
			<key-property name="queryName" column="qry_name"></key-property>
			<key-property name="userID" column="user_id"></key-property>
			<key-property name="ApplicationId" column="appl_id"></key-property>
			<key-property name="equipmentInitial" column="eqmt_init"></key-property>
			<key-property name="equipmentNumber" column="eqmt_nbr"></key-property>
		</composite-id>
		<property name="equipmentSortSequence" column="eqmt_sort_seq"></property>
	</class>

</hibernate-mapping>